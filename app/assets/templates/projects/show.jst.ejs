
<button class="back">Back</button>
<button class="edit-mode">Edit Mode</button>
<button class="delete">Delete</button>

<h1><%= model.escape("title") %></h1>

<p><%= model.escape("description") %></p>

<p><%= model.escape("year") %></p>


<% model.photos().each(function(photo){ %>

	<a href="<%= photo.url() %>"><img src="<%= photo.get("avatar_styles").full %>"></a>

<% }) %>


<a href="/projects/<%= model.get("id") %>/photos/new">Link to Picture Upload!</a>



<h3>Tags</h3>
<% model.media().each(function(medium){ %>

<li><a href="#/media/<%= medium.get("id") %>"><%= medium.escape("name") %></a></li>

<% }) %>
