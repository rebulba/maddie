<!DOCTYPE html>
<html>
<head>
  <title>Maddie</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	
  <% if flash[:errors] %>
  	<ul class="error-list">
    <% flash[:errors].each do |error| %>
      <li class="error"><%= error %></li>
    <% end %>
    	</ul>
  <% end %>

<%= yield %>

</body>
</html>
