<div class="edit-index">
  <%= content_for :dialogue do %>
    <h1>Click to Edit</h1>
  <% end %>

  <div class="welcome container-fluid">
    <ul class="pages nav nav-stacked">
      <% @pages.each do |page| %>
        <li>
          <%= link_to(
            page.title,
            edit_page_url(page)
          ) %>
        </li>
      <% end %>

      <li>
        <%= link_to "New Page", new_page_path  %>
      </li>

      <li>
        <%= link_to "Swap Order", new_order_swap_path  %>
      </li>
    </ul>
  </div>
</div>
